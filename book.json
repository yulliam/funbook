{
	"title": "Yulliam's Doc",
    "author": "Yulliam",
    "description": "record some thing",
    "language": "zh-hans",
	"plugins":[
		"expandable-chapters",
		"-lunr", 
		"-search", 
		"search-pro",
		"anchor-navigation-ex",
		"accordion",
		"edit-link",
		"prism", 
		"-highlight",
		"hide-element",
		"code"
    ],
	"pluginsConfig": {
        "anchor-navigation-ex": {
            "showLevel": false,
            "showGoTop": true
        },
        "edit-link": {
            "base": "https://github.com/yulliam/funbook",
            "label": "编辑此页面"
        },
        "prism": {
			"css": [
				"prismjs/themes/prism-okaidia.css"
			]
		},
		"hide-element": {
            "elements": [".gitbook-link"]
        },
		"code": {
        "copyButtons": false
		}
    },
	"ignores" : ["_book", "node_modules"]
}